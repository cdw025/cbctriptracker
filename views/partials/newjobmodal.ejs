<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="newJobModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="NewJobModalLabel">Create New Job</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form id="newjobform">
            <h5>Job Information</h5>
            <div class="row">
                <div class="col-4">
                    <div class="form-group">
                        <label for="JobNumberInput">Job #</label>
                        <input type="text" class="form-control" id="JobNumberInput" placeholder="Job #">
                    </div>
                </div>
                <div class="col-4">
                  <div class="form-group">
                      <label for="customerNameInput">Customer Name</label>
                      <input type="text" class="form-control" id="customerNameInput" placeholder="Customer Name">
                  </div>
              </div>
                  <div class="col-4">
                      <div class="form-group">
                          <label for="OriginInput">Origin Location</label>
                          <input type="text" class="form-control" id="OriginInput" placeholder="Origin">
                      </div>
              </div>
          </div>
          <div class="row">
            <div class="col-4">
                <div class="form-group">
                    <label for="OriginDescription">Origin Description</label>
                    <input type="text" class="form-control" id="OriginDescription" placeholder="Origin Description">
                </div>
            </div>
            <div class="col-4">
              <div class="form-group">
                  <label for="DestinationInput">Destination Location</label>
                  <input type="text" class="form-control" id="DestinationInput" placeholder="Destination Location">
              </div>
          </div>
              <div class="col-4">
                  <div class="form-group">
                      <label for="DestinationDescription">Destination Description</label>
                      <input type="text" class="form-control" id="DestinationDescription" placeholder="Destination">
                  </div>
          </div>
      </div>
              <div class="row">
                  <div class="col-12">
                      <div class="form-group">
                          <label for="OrderNotes">Order Notes</label>
                          <input type="text" class="form-control" id="OrderNotes" placeholder="Order Notes">
                      </div>
                  </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="submit" class="btn btn-primary" onsubmit="return false">Create New Job</button>
            </div>
        </form>
      </div>
    </div>
  </div>
</div>