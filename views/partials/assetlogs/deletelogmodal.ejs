<div class="modal fade" id="dlog_id<%- log.log_id %>" tabindex="-1" role="dialog" aria-labelledby="deleteLogModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title w-100 text-center">Are you sure you want to delete this log?  This cannot be undone.</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form class="deletelogform">
            <div class="form-group">
              <label hidden for="dlog_id"></label>
              <input hidden type="text" class="form-control dlog_id" value="<%= log.log_id %>">
          </div>
          <table class="table table-bordered text-center">
            <thead>
              <tr>
                <th scope="col">Log Date</th>
                <th scope="col">Asset</th>
                <th scope="col">Status</th>
                <th scope="col">Location</th>
                <th scope="col">Coordinates</th>
                <th scope="col">Direction</th>
                <th scope="col">Miles Made</th>
                <th scope="col">Miles to Go</th>
                <th scope="col">ETA</th>
                <th scope="col">Fuel Burn</th>
                <th></th>
              </tr>
            </thead>
            <tbody>
                <tr>
                    <th scope="row"><%- moment(log.log_dttm).format('M/D/YY @ kk:mm') %></th>
                    <td><%- log.asset_name %></td>
                    <td><%- log.status %></td>
                    <td><%- log.asset_location %></td>
                    <td>(<%- log.latitude %>, <%- log.longitude %>)</td>
                    <td><%- log.direction %></td>
                    <td><%- log.miles_made %></td>
                    <td><%- log.miles_to_go %></td>
                    <td><% if(log.eta === null) { %>
                      - 
                  <%  } else { %>
                      <%- moment(log.eta).format('M/D/YY @ kk:mm') %> 
                    <% } %></td>
                    <td><%- log.fuel_burn %></td>
                </tr>
            </tbody>
          </table>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
          <button type="submit" class="btn btn-danger">DELETE THIS LOG</button>
        </div>
      </form>
      </div>
    </div>
  </div>